        "type": {
          "const": "number",
          "title": "Type",
          "type": "string"
        }
      },
      "required": [
        "type"
      ],
      "title": "ExtraOptionNumberSchemaType",
      "type": "object"
    },
    "ExtraOptionStringSchemaType": {
      "additionalProperties": false,
      "description": "String extra option schema.",
      "properties": {
        "const": {
          "title": "Const",
          "type": "string"
